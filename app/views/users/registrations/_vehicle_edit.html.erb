<div id="vehicle-edit">
<%= form_for(@user) do |f| %>
  <%= devise_error_messages! %>

    <div class="licenseplates">
      <%= f.fields_for :licenseplates do |g| %>
        <%= render 'users/licenseplate_fields', f: g %>
      <% end %>
    </div>

    <div class="actions">
      <%= f.submit "Update", class: "btn" %>
      <%= link_to_add_association 'Add License Plate', f, :licenseplates, class: "btn", data: 
          { association_insertion_node: '.licenseplates', association_insertion_method: :append } %>
    </div>
</div>

  
<% end %>